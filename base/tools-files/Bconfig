menu "Files built in the toolchain"

config BASE_TOOLS_FILES_MUSL_LIBC
	bool "Musl's libc.so and ld.so"
	depends on TOOLCHAIN_LIBC_MUSL
	help
	  This option includes libc.so provided by musl.

config BASE_TOOLS_FILES_LIBGCC_S
	bool "GCC's libgcc_s.so.*"
	help
	  This option includes libgcc_s.so.* provided by GCC.
endmenu
